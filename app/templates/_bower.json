{
  "name": "<%= _.slugify(appName) %>",
  "version": "0.0.0",
  "dependencies": {
      "vue": null,
      "jquery": null,
      "json3": null,<% if (moduleFramework === 'requirejs') { %>
      "requirejs": null,<% } %>
      "es5-shim": null
  },
  "exportsOverride": {
      "vue": {
          "vendor": "dist/vue*.js"
      },
      "jquery": {
          "vendor": "jquery.min.js"
      },
      "json3": {
          "vendor": "**/*.js"
      },<% if (moduleFramework === 'requirejs') { %>
      "requirejs": {
          "vendor": "require.js"
      },<% } %>
      "es5-shim": {
          "vendor": "*.js"
      }
  }
}

