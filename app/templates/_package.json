        "grunt-svgmin": "latest",
        "grunt-contrib-copy": "latest",
        "grunt-contrib-clean": "latest",
        "grunt-contrib-sass": "latest",
        "grunt-contrib-watch": "latest",<% if (moduleFramework === 'requirejs') { %>
